{
  "name": "deepseek",
  "type": "service",
  "routes": [
    {
      "name": "generate",
      "route": "/generate",
      "batchable": false,
      "input": {
        "properties": {
          "prompt": {
            "default": "Who are you? Please respond in pirate speak!",
            "title": "Prompt",
            "type": "string"
          },
          "max_tokens": {
            "default": 2048,
            "maximum": 2048,
            "minimum": 128,
            "title": "Max Tokens",
            "type": "integer"
          }
        },
        "title": "Input",
        "type": "object"
      },
      "output": {
        "type": "string",
        "is_stream": true,
        "media_type": "text/event-stream"
      },
      "is_task": false
    }
  ]
}